<!-- -*- mode: XML -*- args="1" -->

<launch>


<arg name="bag_path" value="/home/howstar/data/rfans_data/corridor1_double.bag" />


<!-- -->
<node pkg="rfans_merge" type="rfans_merge_node" name="rfans_merge_node" >
  <param name="x16" value="0.0" />
  <param name="y16" value="0.0" />
  <param name="z16" value="0.0" />
  <param name="x32" value="0.0" />
